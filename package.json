{
  "name": "style-compare",
  "version": "0.1.0",
  "homepage": "https://geops.github.io/style-compare",
  "keywords": [
    "compare",
    "mapbox",
    "mobility-toolbox-js",
    "react",
    "react-spatial",
    "styles"
  ],
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "abortcontroller-polyfill": "1.7.5",
    "i18next": "23.2.3",
    "jest-canvas-mock": "2.5.2",
    "mapbox-gl": "1.13.2",
    "maplibre-gl": "3.1.0",
    "mobility-toolbox-js": "2.0.0-beta.82",
    "ol": "7.4",
    "prop-types": "15.8.1",
    "query-string": "^8.1.0",
    "react": "18",
    "react-dom": "18",
    "react-i18next": "13.0.1",
    "react-icons": "4.10.1",
    "react-redux": "8.1.1",
    "react-spatial": "1.5.2",
    "redux": "4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fixpack": "4.0.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest-serializer-html": "7.1.0",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "react-scripts": "5.0.1",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.4.2",
    "sass": "1.63.6",
    "stylelint": "15.9.0",
    "stylelint-config-recommended-scss": "12.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-scss": "5.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "predeploy": "yarn build",
    "prepare": "is-ci || husky install",
    "start": "ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(@geops/react-ui|jsts|ol|ol-mapbox-style|ole|filter-obj|react-spatial|mobility-toolbox-js|quick-lru|split-on-first|query-string|decode-uri-component)/).*/\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true,
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "cypress",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "lint-staged": {
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "yarn test --bail --findRelatedTests"
    ],
    "package.json": [
      "fixpack --sortToTop name --sortToTop description --sortToTop version --sortToTop author --sortToTop homepage --sortToTop keywords  --sortToTop main --sortToTop proxy --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop resolutions --sortToTop scripts",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ]
  }
}
